<h1>Create User</h1>

<%= erb :'_errors' %>

<div class='form'>
<form action='/users' method='Post'>
  <div><input type='text' name='user[username]' placeholder="Username"></div></br>
  <div><input type='text' name='user[email]' placeholder="Email address"></div></br>
  <div><input type='password' name='user[password]' placeholder="Password"></div></br>
  <div><input type='submit' value='Create User'></div>
</form>
</div>

